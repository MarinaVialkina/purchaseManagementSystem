{
  "name": "@reatom/lens",
  "version": "3.12.0",
  "private": false,
  "sideEffects": false,
  "description": "FRP operators and other utilities for the Reatom state manager",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.js",
    "default": "./build/index.mjs"
  },
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "yoctobundle",
    "test": "vitest run src/*",
    "test:watch": "vitest src/*"
  },
  "dependencies": {
    "@reatom/core": "^3.4.0",
    "@reatom/effects": "^3.2.0",
    "@reatom/hooks": "^3.3.1",
    "@reatom/primitives": "^3.6.0",
    "@reatom/utils": "^3.1.0"
  },
  "devDependencies": {
    "@reatom/npm-zod": "^3.10.3"
  },
  "author": "artalar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/package/lens",
  "files": [
    "/build"
  ]
